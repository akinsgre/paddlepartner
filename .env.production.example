# Production environment variables for AWS deployment
# Copy this file to .env.production and fill in your actual values

# API Configuration
VITE_API_BASE_URL=https://your-backend-url.amazonaws.com/api
VITE_CLIENT_URL=https://your-frontend-url.amplifyapp.com

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Strava API Configuration  
VITE_STRAVA_CLIENT_ID=your-strava-client-id
VITE_STRAVA_REDIRECT_URI=https://your-frontend-url.amplifyapp.com/auth/strava/callback

# Environment
NODE_ENV=production